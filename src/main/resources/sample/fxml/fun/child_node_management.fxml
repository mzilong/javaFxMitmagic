<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import sample.view.*?>

<AnchorPane style="-fx-background-color: -fx-background" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controller.MainController">
    <children>
        <VBox fx:id="vboxNode" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <AnchorPane fx:id="apNode">
                    <children>
                        <GridPane fx:id="gpNode" hgap="5.0" vgap="5.0">
                            <columnConstraints>
                                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
                                <ColumnConstraints />
                            </columnConstraints>
                            <rowConstraints>
                                <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                                <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                            </rowConstraints>
                            <children>
                                <Label text="%ChildNodeQuery.StartNum">
                                    <graphic>
                                        <Icon styleClass="el-icon-sunrise-1" />
                                    </graphic>
                                </Label>
                                <TextField fx:id="tfStartNum" text="0" GridPane.columnIndex="1" />
                                <Label text="%ChildNodeQuery.PageNum" GridPane.columnIndex="0" GridPane.halignment="LEFT" GridPane.rowIndex="1">
                                    <graphic>
                                        <Icon styleClass="el-icon-moon-night" />
                                    </graphic>
                                </Label>
                                <TextField fx:id="tfPageNum" text="16" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                            </children>
                        </GridPane>
                    </children>
                </AnchorPane>
                <AnchorPane fx:id="apNodeResult">
                    <children>
                        <TextArea fx:id="labNodeResult" editable="false" prefHeight="350.0" AnchorPane.bottomAnchor="0" AnchorPane.leftAnchor="0" AnchorPane.rightAnchor="0" AnchorPane.topAnchor="0">
                            <font>
                                <Font name="System Bold" size="13.0" />
                            </font>
                        </TextArea>
                    </children>
                </AnchorPane>
            </children>
        </VBox>
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
</AnchorPane>
