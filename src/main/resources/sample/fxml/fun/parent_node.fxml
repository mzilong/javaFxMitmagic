<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import sample.view.*?>

<AnchorPane style="-fx-background-color: -fx-background" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controller.MainController">
    <children>
        <VBox fx:id="vboxParentNode" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <HBox fx:id="hboxParentNode" spacing="5.0">
                    <children>
                        <Label maxHeight="1.7976931348623157E308" text="%ParentNode.Addr">
                            <graphic>
                                <Icon styleClass="el-icon-edit" />
                            </graphic>
                        </Label>
                        <TextField fx:id="tfParentNode" />
                    </children>
                </HBox>
                <AnchorPane fx:id="apParentNodeResult">
                    <children>
                        <TextArea fx:id="labParentNodeResult" editable="false" prefHeight="100.0" AnchorPane.bottomAnchor="0" AnchorPane.leftAnchor="0" AnchorPane.rightAnchor="0" AnchorPane.topAnchor="0">
                           <font>
                              <Font name="System Bold" size="13.0" />
                           </font>
                        </TextArea>
                    </children>
                </AnchorPane>
            </children>
        </VBox>
    </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</AnchorPane>
