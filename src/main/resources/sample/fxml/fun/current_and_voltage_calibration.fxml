<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import sample.view.*?>

<AnchorPane style="-fx-background-color: -fx-background" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controller.MainController">
    <children>
        <VBox fx:id="vboxCalibration" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <AnchorPane fx:id="apCalibration">
                    <children>
                        <GridPane fx:id="gpCalibration" hgap="5.0" vgap="5.0">
                            <columnConstraints>
                                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
                                <ColumnConstraints />
                        <ColumnConstraints />
                        <ColumnConstraints />
                        <ColumnConstraints />
                            </columnConstraints>
                            <rowConstraints>
                                <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                                <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                        <RowConstraints />
                        <RowConstraints />
                            </rowConstraints>
                            <children>
                                <Label text="%password">
                                    <graphic>
                                        <Icon styleClass="el-icon-key" />
                                    </graphic>
                                </Label>
                                <TextField fx:id="tfPassword" maxWidth="100.0" text="000" GridPane.columnIndex="1" />
                                <Label text="%CurrentProportionalCoefficientCompensation" GridPane.rowIndex="1">
                                    <graphic>
                                        <Icon styleClass="el-icon-minus" />
                                    </graphic>
                                </Label>
                                <TextField fx:id="tfACurrentNum" maxWidth="100.0" promptText="A" text="0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                                <TextField fx:id="tfBCurrentNum" maxWidth="100.0" promptText="B" text="0" GridPane.columnIndex="2" GridPane.rowIndex="1" />
                                <TextField fx:id="tfCCurrentNum" maxWidth="100.0" promptText="C" text="0" GridPane.columnIndex="3" GridPane.rowIndex="1" />
                                <TextField fx:id="tfNCurrentNum" maxWidth="100.0" promptText="N" text="0" GridPane.columnIndex="4" GridPane.rowIndex="1" />
                                <Label text="%VoltageScalingFactorCompensation" GridPane.columnIndex="0" GridPane.halignment="LEFT" GridPane.rowIndex="2">
                                    <graphic>
                                        <Icon styleClass="el-icon-attract" />
                                    </graphic>
                                </Label>
                                <TextField fx:id="tfAVoltageNum" maxWidth="100.0" promptText="A" text="0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                                <TextField fx:id="tfBVoltageNum" maxWidth="100.0" promptText="B" text="0" GridPane.columnIndex="2" GridPane.rowIndex="2" />
                                <TextField fx:id="tfCVoltageNum" maxWidth="100.0" promptText="C" text="0" GridPane.columnIndex="3" GridPane.rowIndex="2" />
                            </children>
                        </GridPane>
                    </children>
                </AnchorPane>
                <AnchorPane fx:id="apCalibrationResult">
                    <children>
                        <TextArea fx:id="labCalibrationResult" editable="false" prefHeight="350.0" AnchorPane.bottomAnchor="0" AnchorPane.leftAnchor="0" AnchorPane.rightAnchor="0" AnchorPane.topAnchor="0">
                            <font>
                                <Font name="System Bold" size="13.0" />
                            </font>
                        </TextArea>
                    </children>
                </AnchorPane>
            </children>
        </VBox>
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
</AnchorPane>
